#                                                                        #
#   Function F_3DFACE :              Status: Implemented                 #
#                                                                        #
#   Author : FW                      Creation      : 17/06/2003          #
#                                                                        #
#                                    Modifications : 17/06/2003  FW      #
#                                                                        #
#   In  : NbPoint, Integer, Glopbal                                      #
#         NbCurve, Integer, Glopbal                                      #
#         NbSurface, Integer, Glopbal                                    #
#         ReadValue , Integer, Global                                    #
#                                                                        #
#   Out : NbPoint, Integer, Glopbal                                      #
#         NbCurve, Integer, Glopbal                                      #
#         NbSurface, Integer, Glopbal                                    #
#         ReadValue , Integer, Global                                    #
#         code, list of string, Local                                    #
#                                                                        #
#   Description :                                                        #
#      The F_3DFACE function convert the 3DFACE entity of the dxf format #
#      to the planar surface object of the geo format. 3 or 4 points in  #
#      the dxf format are converted in 3 or 4 points in the geo format,  #
#      then create the line between the points and the plane surface     #
#      based on these lines.                                             #
#                                                                        #
#   Comments :                                                           #
#      In the 3DFACE section we read the line until we find the value    #
#      '0' whih is the end of section. If the read line is '10','20',    #
#      '30', '11', '21', '31', '12', '22', '32', '13', '23' or '33': the #
#      line after is a coordinate of one of the 4 points. All the other  #
#      lines are not significant. If the 4th point is equal to the third #
#      one, then the geometrical object is a triangle and not a          #
#      quadrilateral                                                     #
#                                                                        #
##########################################################################
                                         #
def F_3DFACE():                          # F_3DFACE Function  
                                         #
   global NbPoint                        # Declare NbPoint scope
   global NbCurve                        # Declare NbCurve scope 
   global NbLoop                         # Declare NbLoop scope 
   global NbSurface                      # Declare NbSurface scope   
   global ReadValue                      # Declare ReadValue scope
   global PointList                      # Declare Point scope
   global LineList                       # Declare Line scope
   global FaceList                       # Declare Face scope
   if Debug == 1:                        # For Debug
      print 'F_3DFACE() function Start'  # 
   ReadValue = 1                         # Variable has to be read in the
                                         #  Value_Cmd function
   code = []                             # Init return code
   code.append('1')                      # Fisrt element of code is a 
                                         #  return code which indicates 
                                         #  which treatment has to be done
                                         #  in the main loop after the 
                                         #  call of the present function
                                         #  1 means write in the output
                                         #  file, 0 means do nothing
   Value = []                            # Init Value List
   NumPoint = []                         # Init NumPoint List
   NumLine = []                          # Init NumLine List
   line = ReadLine()                     # Read the next line
   while line <> '0':                    # While line is different to '0'
      ret = AC_KeyWord[line]()           #  call the corresponding 
                                         #   function 
      if ret <> '0':                     #  if the return of the function 
         Value.append(ret)               #   is different to 0 then update
      if line == '33':                   #  If line = '33' ReadValue = 0 
         ReadValue = 0                   #   We have all the coordinates,
                                         #   we don't need to read more 
                                         #   values 
      line = ReadLine()                  #   the Value List.
   j = 0                                 # Init counter
   for i in range(4):                    # Create the 4 string for the
      TempPoint = [str(Value[i*2+j]),    # Temporary Point
                   str(Value[i*2+1+j]),  #
                   str(Value[i*2+2+j])]  #
      if TempPoint not in PointList:     # 
         code.append('Point('+           #  geo format corresponding to
               str(NbPoint)+             #  the 4 points. for 1 point,
               ') = {'+str(Value[i*2+j])+#  string is: 'Point(PointNunber)
               ', '+str(Value[i*2+1+j])+ #  'Point(PointNunber) = {XValue,
               ', '+str(Value[i*2+2+j])+ #   YValue, ZValue};'
               ', '+str(Refine)+'};\n')  #   
         NumPoint.append(NbPoint)        #  Save the point Number in the
                                         #   NumPoint List
         NbPoint = NbPoint+1             #
         PointList.append([              #
                    str(Value[i*2+j]),   #  
                    str(Value[i*2+1+j]), #  'Point(PointNunber) = {XValue,
                    str(Value[i*2+2+j])])#
      else:                              #
         NumPoint.append(PointList.index(#
                   TempPoint)+1)         #
      j = j+1                            #  Update counter
   i = 1                                 # Init counter
   for Val in NumPoint:                  # Create the line between the
      if i == len(NumPoint):             #
         TempLine =[NumPoint[i-1],       #
                    NumPoint[0]]         # 
      else:                              #
         TempLine =[NumPoint[i-1],       #
                    NumPoint[i]]         #
      TempLine.sort()                    #          
      if ((TempLine not in LineList) and #
          (TempLine[0] <> TempLine[1])): #
         code.append('Line('+            # 'Line(LineNumber) = {Point A,
                str(NbCurve)+            #  Point B};'
                ') = {'+str(TempLine[0])+#
                ', '+str(TempLine[1])+   #
                '};\n')	                 #
         NumLine.append(NbCurve)         #  Save the line number in the
                                         #   NumLine List
         LineList.append(TempLine)       #
         NbCurve = NbCurve+1             #
      elif TempLine[0] <> TempLine[1]:   #  
         NumLine.append(LineList.index(  #  Save the line number in the
                        TempLine)+1)     #   NumLine List
      i = i+1                            #  Update Counter
   TempFace = []                         #
   if len(NumLine) > 2:                  #
      for Num in NumLine:                #
         TempFace.append(Num)            #
      TempFace.sort ()                   #
      if TempFace not in FaceList:       #
         if LineList[NumLine[0]-1][0] in LineList[NumLine[1]-1]:
            TempNumLine = [-NumLine[0]]
         else: 
            TempNumLine = [NumLine[0]]
         for i in range(1,len(NumLine)): 
            if TempNumLine[i-1] < 1:
               EndPoint = LineList[abs(TempNumLine[i-1])-1][0]
            else: 
               EndPoint = LineList[TempNumLine[i-1]-1][1]
            if LineList[NumLine[i]-1][0] <> EndPoint: 
               TempNumLine.append(-NumLine[i])
            else:
               TempNumLine.append(NumLine[i])
         TempLine = str(TempNumLine[0])  # Create the plane surface 
         for Val in TempNumLine[1:]:     #  ('Plane Surface(Surface 
            TempLine = (TempLine+', '+   #    Number) = {Line Loop N};')
                        str(Val))        #
         code.append('Line Loop('+       #  which is based on the Line 
                 str(NbLoop)+            #  Loop N ('Line Loop(N) = {
                 ') = {'+TempLine+'};\n')#  Line 1, Line 2,...,Line n};')
         code.append('Plane Surface('+   #  which define the wire of the
                   str(NbSurface)+       #  plane surface
                   ') = {'+              #
                   str(NbSurface)+'};\n')#  
         NbSurface = NbSurface+1         # Update NbSurface
         NbLoop = NbLoop+1               # Update NbLoop
         FaceList.append(TempFace)       #
   if Debug == 1:                        # For Debug
      print 'F_3DFACE() function End'    # 
   return code                           #

